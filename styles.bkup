.highlight {
  font-style: bold;
  font-family: Arial, Helvetica, sans-serif;
  text-align: center;
  color: white;
}
.column-left {
  width: 100%;
  float: left;
}
.column-right {
  width: 80%;
  float: right;
  padding: 10px;
  box-sizing: border-box;
  color: white;
}
.picture {
  float: left;
  padding: 10px;
  background-color: grey;
  display: block;
  width: 20%;
  box-sizing: border-box;
}
.box {
  float: right;
  width: 80%;
  padding: 10px;
  box-sizing: border-box;
  color: white;
}
.box h2 {
  float: right;
  width: 100%;
  padding: 5px;
  border-bottom: solid white;
  margin-top: auto;
  box-sizing: border-box;
  font-family: Arial, Helvetica, sans-serif;
}
.special {
  font-weight: bold;
}
body {
  background: black;
}